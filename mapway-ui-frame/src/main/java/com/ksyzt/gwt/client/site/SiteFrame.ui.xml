<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="style" type="com.ksyzt.gwt.client.common.CssStyle">
	</ui:with>
	<g:DockLayoutPanel ui:field="root">
		<g:north size="60">
			<g:HorizontalPanel verticalAlignment="middle" spacing="4" styleName="{style.top}" width="100%">
				<g:Label text="系统配置"></g:Label>
				<g:Button ui:field="btnQiut" text="退出"></g:Button>
			</g:HorizontalPanel>
		</g:north>
		<g:west size="250">
			<g:ScrollPanel styleName="{style.left_panel}" width="100%">
				<g:VerticalPanel width="100%" spacing="5">
					<g:Button ui:field="btnSiteInfo" text="站点信息"></g:Button>
					<g:Button ui:field="btnURL" text="静态映射"></g:Button>
				</g:VerticalPanel>
			</g:ScrollPanel>
		</g:west>
	</g:DockLayoutPanel>
</ui:UiBinder> 