<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.ksyzt.gwt.client.common">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="site-app-topbar">
		<table width="100%">
			<tr>
				<td rowspan="2">
					<div class="site-app-topbar-logo"></div>
				</td>
				<td>
					<g:HorizontalPanel styleName="site-app-topbar-user-panel"
						spacing="1" verticalAlignment="middle">
						<g:Label ui:field="lbUserName" styleName="site-app-topbar-username"></g:Label>
						<g:cell horizontalAlignment="right">
							<g:Anchor ui:field="linkQuit" styleName='btn_exit'></g:Anchor>
						</g:cell>

					</g:HorizontalPanel>
				</td>
			</tr>
			<tr>
				<td>
					<div class='site-app-topbar-tabs'>
						<c:CustomTab ui:field="tabs">
						</c:CustomTab>
					</div>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 