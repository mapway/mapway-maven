<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
      .left{
	padding: 20px;
	text-align: left;
	position: fixed;
	float: left;
}
.right{
	margin-left: 220px;
}
    </ui:style>
   <g:HTMLPanel>
            <g:HTMLPanel ui:field="navi" styleName="{style.left}">
        </g:HTMLPanel>
            <g:HTMLPanel ui:field="list" styleName="{style.right}">
        
            </g:HTMLPanel>    
        
   </g:HTMLPanel>
         
    
</ui:UiBinder>